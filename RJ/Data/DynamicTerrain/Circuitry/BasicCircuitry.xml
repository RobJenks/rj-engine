<gamedata>

  <!-- 
    Name: Data relay 
    Description: Simply passes input signals to its output port without modification
    Inputs: 1
    Outputs: 1
  -->
  <DynamicTerrainDefinition code="DataObjectRelay" class="DataObjectRelay" def="tmp_terrain_cone">
    <DefaultSuccessfulInteractionAudio name="electronic_switch_use" volume="1.0" />
    <DefaultFailedInteractionAudio name="electronic_switch_fail_to_use" volume="1.0" />
  </DynamicTerrainDefinition>

  
  <!-- 
    Name: Debug output component
    Description: Raises a debug message for each signal received at the component input
    Inputs: 1
    Outputs: 0
  -->
  <DynamicTerrainDefinition code="DataObjectDebugLogger" class="DataObjectDebugLogger" def="tmp_terrain_cone">
  </DynamicTerrainDefinition>

  

</gamedata>