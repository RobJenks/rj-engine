<gamedata>
  <!-- Load the universe, which will in turn load all systems and system data -->
  <include file="Universe.xml" />

  <!-- Load all game resources and production data -->
  <include file="Resources\Resources.xml" />
  
  <!-- Load all factions -->
  <include file="Factions\Factions.xml" />

  <!-- Load modifier data -->
  <include file="Modifiers\Modifiers.xml" />

  <!-- General rendering resources -->
  <include file="Rendering\emissive_materials.xml" />
  <include file="Rendering\misc_rendering_materials.xml" />
  <include file="Rendering\Noise\noise_resources.xml" />

  <!-- Load shared static model data -->
  <include file="Models\BasicModels.xml" />
  <include file="Ships\TileModels.xml" />

  <!-- Load articulated model data (must be loaded after static models) -->
  <include file="Models\Turrets\TurretModels.xml" />
  <include file="Models\Switches\SwitchModels.xml" />

  <!-- Load all audio data -->
  <include file="Audio\Audio.xml" />
  
  <!-- Load terrain objects -->
  <include file="Terrain\Terrain.xml" />

  <!-- Load all dynamic terrain definitions.  Must be loaded after basic terrain -->
  <include file="DynamicTerrain\DynamicTerrain.xml" />
  
  <!-- Load turret object data (must load Projectiles > Launchers > Turrets due to dependencies) -->
  <include file="Projectiles\ProjectileDefinitions.xml" />
  <include file="Turrets\ProjectileLaunchers.xml" />
  <include file="Turrets\Turrets.xml" />
  
  <!-- Load equipment data-->
  <include file="Engines.xml" />

  <!-- Load effects and particle emitter data -->
  <include file="Effects\Effects.xml" />
  <include file="Particles\ParticleEmitters.xml" />

  <!-- Load fonts -->
  <include file="Fonts\Fonts.xml" />

  <!-- Load overlay renderer resources -->
  <include file="Overlay\OverlayResources.xml" />
  
  <!-- Load ship data -->
  <include file="Ships\Ships.xml" />

  <!-- Load the set of ship tile classes, then the set of tile definitions -->
  <include file="Ships\ComplexShipTileClasses.xml" />
  <include file="Ships\ComplexShipTileDefinitions.xml" />

  <!-- Load the set of all standard complex ship sections -->
  <include file="Ships\ComplexShipSections.xml" />

  <!-- Load all complex ships in the game and their associated sections -->
  <include file="Ships\ComplexShips.xml" />

  <!-- Load all custom user ships from the relevant source directory -->
  <include file="Ships\CustomDesigns" />

  <!-- Load ship loadouts, after both the ships and equipement/objects to be mapped -->
  <include file="Ships\SimpleShipLoadouts.xml" />
  
  <!-- Load user interface data -->
  <include file="UI\UserInterface.xml" />
  
  <!-- Load data on all skinned actor models in the game.  Sequencing is importaant -->
  <include file="Actors\SkinnedModels.xml" />
  <include file="Actors\ActorAttributeGeneration.xml" />
  <include file="Actors\Actors.xml" />
  
</gamedata>